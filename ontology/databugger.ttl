#################################################################
#
#    Object Properties
#
#################################################################


###  http://databugger.aksw.org/ontology#appliesTo

tddo:appliesTo rdf:type owl:ObjectProperty ;
               
               rdfs:comment "applies to describes the ontology used for terms in the Test." ;
               
               rdfs:domain tddo:Test ;
               
               rdfs:range tddo:TestAppliesTo .



###  http://databugger.aksw.org/ontology#basedOnPattern

tddo:basedOnPattern rdf:type owl:ObjectProperty ;
                    
                    rdfs:comment "basedOnPattern contains the Data Quality Test Pattern that is instantiated in the described Test." ;
                    
                    rdfs:range tddo:Pattern .



###  http://databugger.aksw.org/ontology#generated

tddo:generated rdf:type owl:ObjectProperty ;
               
               rdfs:comment "generated specifies the way the test was constructed." ;
               
               rdfs:domain tddo:Test ;
               
               rdfs:range tddo:TestGenerationType .



###  http://databugger.aksw.org/ontology#parameter

tddo:parameter rdf:type owl:ObjectProperty ;
               
               rdfs:range tddo:Parameter ;
               
               rdfs:domain tddo:Pattern .



###  http://databugger.aksw.org/ontology#parameterConstraint

tddo:parameterConstraint rdf:type owl:ObjectProperty ;
                         
                         rdfs:domain tddo:Parameter ;
                         
                         rdfs:range tddo:ParameterConstraint .



###  http://databugger.aksw.org/ontology#references

tddo:references rdf:type owl:ObjectProperty ;
                
                rdfs:comment "references specifies the Classes and Properties involved in the described testcase." ;
                
                rdfs:domain tddo:Test .



###  http://databugger.aksw.org/ontology#source

tddo:source rdf:type owl:ObjectProperty ;
            
            rdfs:comment "source is the SPARQL endpoint specified in the testcase." ;
            
            rdfs:domain tddo:Test .





#################################################################
#
#    Data properties
#
#################################################################


###  http://databugger.aksw.org/ontology#generatorSPARQL

tddo:generatorSPARQL rdf:type owl:DatatypeProperty ;
                     
                     rdfs:domain tddo:TestGenerator .



###  http://databugger.aksw.org/ontology#patternPrevalenceSPARQL

tddo:patternPrevalenceSPARQL rdf:type owl:DatatypeProperty ;
                             
                             rdfs:domain tddo:Pattern .



###  http://databugger.aksw.org/ontology#patternSPARQL

tddo:patternSPARQL rdf:type owl:DatatypeProperty ;
                   
                   rdfs:comment "" ;
                   
                   rdfs:domain tddo:Pattern .



###  http://databugger.aksw.org/ontology#selectVariable

tddo:selectVariable rdf:type owl:DatatypeProperty ;
                    
                    rdfs:domain tddo:Pattern .



###  http://databugger.aksw.org/ontology#sparql

tddo:sparql rdf:type owl:DatatypeProperty ;
            
            rdfs:domain tddo:Test .



###  http://www.co-ode.org/ontologies/ont.owl#sparqlPrevalence

<http://www.co-ode.org/ontologies/ont.owl#sparqlPrevalence> rdf:type owl:DatatypeProperty ;
                                                            
                                                            rdfs:domain tddo:Test .





#################################################################
#
#    Classes
#
#################################################################


###  http://databugger.aksw.org/ontology#Parameter

tddo:Parameter rdf:type owl:Class .



###  http://databugger.aksw.org/ontology#ParameterConstraint

tddo:ParameterConstraint rdf:type owl:Class .



###  http://databugger.aksw.org/ontology#Pattern

tddo:Pattern rdf:type owl:Class .



###  http://databugger.aksw.org/ontology#Test

tddo:Test rdf:type owl:Class ;
          
          rdfs:comment "A Test is a testcase in which all properties of the test are described." .



###  http://databugger.aksw.org/ontology#TestAppliesTo

tddo:TestAppliesTo rdf:type owl:Class .



###  http://databugger.aksw.org/ontology#TestGenerationType

tddo:TestGenerationType rdf:type owl:Class .



###  http://databugger.aksw.org/ontology#TestGenerator

tddo:TestGenerator rdf:type owl:Class .





#################################################################
#
#    Individuals
#
#################################################################


###  http://databugger.aksw.org/ontology#Application

tddo:Application rdf:type tddo:TestAppliesTo ,
                          owl:NamedIndividual ;
                 
                 rdfs:comment "Application: When the tests are specific to an application only" .



###  http://databugger.aksw.org/ontology#AutoGenerated

tddo:AutoGenerated rdf:type tddo:TestGenerationType ,
                            owl:NamedIndividual ;
                   
                   rdfs:comment "AutoGenerated: This test was created automatically" .



###  http://databugger.aksw.org/ontology#Class

tddo:Class rdf:type tddo:ParameterConstraint ,
                    owl:NamedIndividual ;
           
           rdfs:comment "The parameter is restricted to a Class." .



###  http://databugger.aksw.org/ontology#Dataset

tddo:Dataset rdf:type tddo:TestAppliesTo ,
                      owl:NamedIndividual ;
             
             rdfs:comment "Dataset: When the tests applies to a dataset only (i.e. a SPARQL Endpoint)" .



###  http://databugger.aksw.org/ontology#EnrichedSchema

tddo:EnrichedSchema rdf:type tddo:TestAppliesTo ,
                             owl:NamedIndividual ;
                    
                    rdfs:comment "EnrichedSchema: When the tests applies to an ontology / vocabulary enriched with external (semi-)automatic approaches" .



###  http://databugger.aksw.org/ontology#ManuallyGenerated

tddo:ManuallyGenerated rdf:type tddo:TestGenerationType ,
                                owl:NamedIndividual ;
                       
                       rdfs:comment "ManuallyGenerated: This test was created manually" .



###  http://databugger.aksw.org/ontology#None

tddo:None rdf:type tddo:ParameterConstraint ,
                   owl:NamedIndividual ;
          
          rdfs:comment "The parameter can be anything, even a free variable (?var)" .



###  http://databugger.aksw.org/ontology#Operator

tddo:Operator rdf:type tddo:ParameterConstraint ,
                       owl:NamedIndividual ;
              
              rdfs:comment "The parameter can be an operator." .



###  http://databugger.aksw.org/ontology#Property

tddo:Property rdf:type tddo:ParameterConstraint ,
                       owl:NamedIndividual ;
              
              rdfs:comment "The parameter is restricted to a property." .



###  http://databugger.aksw.org/ontology#Resource

tddo:Resource rdf:type tddo:ParameterConstraint ,
                       owl:NamedIndividual ;
              
              rdfs:comment "The pattern can be a resource." .



###  http://databugger.aksw.org/ontology#Schema

tddo:Schema rdf:type tddo:TestAppliesTo ,
                     owl:NamedIndividual .




