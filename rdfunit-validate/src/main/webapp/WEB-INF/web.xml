        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>RDFUnit</display-name>
    <servlet>
        <servlet-name>validator</servlet-name>
        <servlet-class>org.aksw.rdfunit.validate.ws.ValidateWS</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>validator</servlet-name>
        <url-pattern>/validate</url-pattern>
    </servlet-mapping>

</web-app>
