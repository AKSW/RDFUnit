 rutt:dataid-dataset-CARD-recom-label
    a       rut:PatternBasedTestCase ;
    dcterms:description "rdfs:label is recommended" ;
    rut:appliesTo rut:Schema ;
    rut:basedOnPattern rutp:TYPRODEP ;
    rut:generated rut:ManuallyGenerated ;
    rut:references rdfs:label ;
    rut:source <http://dataid.dbpedia.org/ns/core#> ;
    rut:binding
        [ a     rut:Binding ;
            rut:bindingValue rdfs:label ;
            rut:parameter rutp:TYPRODEP-P1
        ] ;
    rut:binding
        [ a     rut:Binding ;
            rut:bindingValue prov:Entity ;
            rut:parameter rutp:TYPRODEP-T1
        ] ;
    rut:binding
        [ a     rut:Binding ;
            rut:bindingValue 'NOT' ;
            rut:parameter rutp:TYPRODEP-NOT
        ] ;
    rut:testCaseLogLevel
                  rlog:WARN .

