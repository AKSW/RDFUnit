rutt:dataid-dataset-CARD-recom-label
    a       rut:PatternBasedTestCase ;
    dcterms:description "recommandation for rdfs:label = 1" ;
    rut:appliesTo rut:Schema ;
    rut:basedOnPattern rutp:OWLCARDT ;
    rut:generated rut:ManuallyGenerated ;
    rut:references rdfs:label ;
    rut:source <http://dataid.dbpedia.org/ns/core#> ;
    rut:binding
        [ a     rut:Binding ;
            rut:bindingValue rdfs:label ;
            rut:parameter rutp:OWLCARDT-P1
        ] ;
    rut:binding
        [ a     rut:Binding ;
            rut:bindingValue dataid:Dataset ;
            rut:parameter rutp:OWLCARDT-T1
        ] ;
    rut:binding
        [ a     rut:Binding ;
            rut:bindingValue "!=" ;
            rut:parameter rutp:OWLCARDT-OP
        ] ;
    rut:binding
        [ a     rut:Binding ;
            rut:bindingValue "1";
            rut:parameter rutp:OWLCARDT-V1
        ] ;
    rut:testCaseLogLevel
                  rlog:INFO .

