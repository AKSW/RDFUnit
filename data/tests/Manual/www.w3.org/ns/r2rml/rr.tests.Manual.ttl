                                    ?P1 rdf:type/rdfs:subClassOf* owl:DatatypeProperty .
                                } UNION {
                                    ?P1 rdfs:range ?D1 .
                                    FILTER ( ?D1 in (rdfs:Literal, rdf:langString) ) .
                                } UNION {
                                    ?P1 schema:rangeIncludes ?D1 .
                                    ?D1 rdf:type|rdfs:subClassOf+ schema:DataType .
