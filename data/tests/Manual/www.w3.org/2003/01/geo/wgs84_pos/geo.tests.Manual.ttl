# geo:geometry should be a soft inverseFunctional property
tddt:dbo-INVFUNC-uniquegeometry
      a       tddo:PatternBasedTestCase ;
      tddo:appliesTo tddo:Schema ;
      tddo:basedOnPattern tddp:INVFUNC ;
      tddo:binding
              [ a       tddo:Binding ;
                tddo:bindingValue <http://www.w3.org/2003/01/geo/wgs84_pos#geometry> ;
                tddo:parameter tddp:INVFUNC-P1
              ] ;
      tddo:binding
              [ a       tddo:Binding ;
                tddo:bindingValue "'Resources with the exact same coordinates should be the same'" ;
                tddo:parameter tddp:INVFUNC-MESSAGE
              ] ;
      tddo:generated tddo:ManuallyGenerated ;
      tddo:references <http://www.w3.org/2003/01/geo/wgs84_pos#geometry> ;
      tddo:source <http://www.w3.org/2003/01/geo/wgs84_pos#> ;
      tddo:testCaseLogLevel
              rlog:Warn .
