# geo:geometry should be a soft inverseFunctional property
rutt:dbo-INVFUNC-uniquegeometry
      a       ruto:PatternBasedTestCase ;
      ruto:appliesTo ruto:Schema ;
      ruto:basedOnPattern rutp:INVFUNC ;
      ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue <http://www.w3.org/2003/01/geo/wgs84_pos#geometry> ;
                ruto:parameter rutp:INVFUNC-P1
              ] ;
      ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue "'Resources with the exact same coordinates should be the same'" ;
                ruto:parameter rutp:INVFUNC-MESSAGE
              ] ;
      ruto:generated ruto:ManuallyGenerated ;
      ruto:references <http://www.w3.org/2003/01/geo/wgs84_pos#geometry> ;
      ruto:source <http://www.w3.org/2003/01/geo/wgs84_pos#> ;
      ruto:testCaseLogLevel
              rlog:Warn .
