# geo:lat must be in the range [-90,90]
tddt:dbo-LITRAN-rangegeolatitude
    a       tddo:PatternBasedTestCase ;
    tddo:appliesTo tddo:Schema ;
    tddo:basedOnPattern tddp:LITRAN ;
    tddo:binding
              [ a       tddo:Binding ;
                tddo:bindingValue "[]" ;
                tddo:parameter tddp:LITRAN-T1
              ] ;
    tddo:binding
              [ a       tddo:Binding ;
                tddo:bindingValue <http://www.w3.org/2003/01/geo/wgs84_pos#lat> ;
                tddo:parameter tddp:LITRAN-P1
              ] ;
    tddo:binding
              [ a       tddo:Binding ;
                tddo:bindingValue "!" ;
                tddo:parameter tddp:LITRAN-NOP
              ] ;
    tddo:binding
              [ a       tddo:Binding ;
                tddo:bindingValue "-90" ;
                tddo:parameter tddp:LITRAN-valueMin
              ] ;
    tddo:binding
              [ a       tddo:Binding ;
                tddo:bindingValue "90" ;
                tddo:parameter tddp:LITRAN-valueMax
              ] ;
    tddo:binding
              [ a       tddo:Binding ;
                tddo:bindingValue "'geo:lat must be in the range -90 to 90'" ;
                tddo:parameter tddp:LITRAN-MESSAGE
              ] ;
    tddo:generated tddo:ManuallyGenerated ;
    tddo:references <http://www.w3.org/2003/01/geo/wgs84_pos#lat> ;
    tddo:source <http://www.w3.org/2003/01/geo/wgs84_pos#> ;
    tddo:testCaseLogLevel
            rlog:Error .


