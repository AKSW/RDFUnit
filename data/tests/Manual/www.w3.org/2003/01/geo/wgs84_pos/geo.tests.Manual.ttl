# geo:lat must be in the range [-90,90]
rutt:dbo-LITRAN-rangegeolatitude
    a       ruto:PatternBasedTestCase ;
    ruto:appliesTo ruto:Schema ;
    ruto:basedOnPattern rutp:LITRAN ;
    ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue "[]" ;
                ruto:parameter rutp:LITRAN-T1
              ] ;
    ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue <http://www.w3.org/2003/01/geo/wgs84_pos#lat> ;
                ruto:parameter rutp:LITRAN-P1
              ] ;
    ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue "!" ;
                ruto:parameter rutp:LITRAN-NOP
              ] ;
    ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue "-90" ;
                ruto:parameter rutp:LITRAN-valueMin
              ] ;
    ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue "90" ;
                ruto:parameter rutp:LITRAN-valueMax
              ] ;
    ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue "'geo:lat must be in the range -90 to 90'" ;
                ruto:parameter rutp:LITRAN-MESSAGE
              ] ;
    ruto:generated ruto:ManuallyGenerated ;
    ruto:references <http://www.w3.org/2003/01/geo/wgs84_pos#lat> ;
    ruto:source <http://www.w3.org/2003/01/geo/wgs84_pos#> ;
    ruto:testCaseLogLevel
            rlog:Error .


