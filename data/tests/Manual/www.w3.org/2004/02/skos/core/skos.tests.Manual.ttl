# multiple languages in prefLabel (en)
rutt:skos-ONELANG-singleEnPrefLabel
      a       ruto:PatternBasedTestCase ;
      ruto:appliesTo ruto:Schema ;
      ruto:basedOnPattern rutp:ONELANG ;
      ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue <http://www.w3.org/2004/02/skos/core#prefLabel> ;
                ruto:parameter rutp:ONELANG-P1
              ] ;
      ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue "en" ;
                ruto:parameter rutp:ONELANG-V1
              ] ;
      ruto:binding
              [ a       ruto:Binding ;
                ruto:bindingValue "'skos:prefLabel should have one value for English'" ;
                ruto:parameter rutp:ONELANG-MESSAGE
              ] ;
      ruto:generated ruto:ManuallyGenerated ;
      ruto:references <http://www.w3.org/2004/02/skos/core#prefLabel> ;
      ruto:source <http://www.w3.org/2004/02/skos/core#> ;
      ruto:testCaseLogLevel
              rlog:Warn .


