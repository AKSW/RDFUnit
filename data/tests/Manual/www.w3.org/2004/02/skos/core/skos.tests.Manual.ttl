# Loose Concepts

# AmbiguousprefLabelvalues

# skos:prefLabel cannot exists more than once 
rutt:skos-CARD-prefLabel
      a       rut:PatternBasedTestCase ;
      dcterms:description "skos:prefLabel should exist once for every resource";
      rut:appliesTo rut:Schema ;
      rut:basedOnPattern rutp:OWLCARD ;
      rut:binding
              [ a       rut:Binding ;
                rut:bindingValue <http://www.w3.org/2004/02/skos/core#prefLabel> ;
                rut:parameter rutp:OWLCARD-P1
              ] ;
      rut:binding
              [ a       rut:Binding ;
                rut:bindingValue ">" ;
                rut:parameter rutp:OWLCARD-OP
              ] ;
      rut:binding
              [ a       rut:Binding ;
                rut:bindingValue "1" ;
                rut:parameter rutp:OWLCARD-V1
              ] ;
      rut:generated rut:ManuallyGenerated ;
      rut:references <http://www.w3.org/2004/02/skos/core#prefLabel> ;
      rut:source <http://www.w3.org/2004/02/skos/core#> ;
      rut:testCaseLogLevel
              rlog:Warn .

