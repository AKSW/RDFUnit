# multiple languages in prefLabel (en)
rutt:skos-ONELANG-singleEnPrefLabel
      a       rut:PatternBasedTestCase ;
      dcterms:description "skos:prefLabel should have one value for English";
      rut:appliesTo rut:Schema ;
      rut:basedOnPattern rutp:ONELANG ;
      rut:binding
              [ a       rut:Binding ;
                rut:bindingValue <http://www.w3.org/2004/02/skos/core#prefLabel> ;
                rut:parameter rutp:ONELANG-P1
              ] ;
      rut:binding
              [ a       rut:Binding ;
                rut:bindingValue "en" ;
                rut:parameter rutp:ONELANG-V1
              ] ;
      rut:generated rut:ManuallyGenerated ;
      rut:references <http://www.w3.org/2004/02/skos/core#prefLabel> ;
      rut:source <http://www.w3.org/2004/02/skos/core#> ;
      rut:testCaseLogLevel
              rlog:Warn .


