# Wrong character casing in yago uri's
tddt:dbpedia-org-MATCH-yagocharactercasing
    a     tddo:PatternBasedTestCase ;
    tddo:appliesTo tddo:Dataset ;
    tddo:basedOnPattern tddp:MATCH ;
    tddo:binding
        [ a     tddo:Binding ;
            tddo:bindingValue <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
            tddo:parameter tddp:MATCH-P1
        ] ;
    tddo:binding
        [ a     tddo:Binding ;
            tddo:bindingValue " " ;
            tddo:parameter tddp:MATCH-OP
        ] ;
    tddo:binding
        [ a     tddo:Binding ;
            tddo:bindingValue """^http://dbpedia.org/class/yago/.[a-z][A-Z][A-Z].$""" ;
            tddo:parameter tddp:MATCH-REGEX
        ] ;
    tddo:binding
        [ a     tddo:Binding ;
            tddo:bindingValue "'Wrong capitalization in yago class'" ;
            tddo:parameter tddp:MATCH-MESSAGE
        ] ;
    tddo:generated tddo:ManuallyGenerated ;
    tddo:references <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
    tddo:source <http://dbpedia.org> ;
    tddo:testCaseLogLevel
              rlog:Warn .

# Resource has dbp:dateOfBirth but not dbo:birthDate (possible extraction issue)
tddt:dbpedia-org-manual-dateofbirthbirthdate
    a tddo:ManualTestCase ;
    tddo:appliesTo tddo:Dataset ;
    tddo:generated tddo:ManuallyGenerated ;
    tddo:references <http://dbpedia.org/ontology/birthDate> ;
    tddo:references <http://dbpedia.org/property/dateOfBirth>  ;
    tddo:source <http://dbpedia.org> ;
    tddo:sparqlWhere """ {
                                 ?resource <http://dbpedia.org/property/dateOfBirth> ?v1 .
                                 FILTER NOT EXISTS {?resource <http://dbpedia.org/ontology/birthDate> ?v2}
                                 BIND ('A dbp:birthDate exists and not a dbo:dateOfBirth, possible extraction error or missing mapping' AS ?message)} """ ;
    tddo:sparqlPrevalence """ SELECT (count(?resource) AS ?total) WHERE {
                                 ?resource <http://dbpedia.org/property/dateOfBirth> ?v1 . } """ ;
    tddo:testCaseLogLevel
              rlog:Warn .

