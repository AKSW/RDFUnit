<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:tddo="http://databugger.aksw.org/ontology#"
   xmlns:xml="http://www.w3.org/XML/1998/namespace"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
   xmlns="http://www.semanticweb.org/owl/owlapi/turtle#">
  <owl:Ontology rdf:about="http://databugger.aksw.org/ns/core#">
    <dc:creator xml:lang="en">Dimitris Kontokostas</dc:creator>
    <dc:creator xml:lang="en">Jens Lehmann</dc:creator>
    <dc:creator xml:lang="en">Patrick Westphal</dc:creator>
    <dc:creator xml:lang="en">Roland Cornelissen</dc:creator>
    <dc:creator xml:lang="en">Sebastian Hellmann</dc:creator>
    <dc:creator xml:lang="en">SÃ¶ren Auer</dc:creator>
    <dc:date>2013-12-14</dc:date>
    <dc:description xml:lang="en">The Databugger ontology describes concepts used in Databugger, a test driven data-debugging framework that can run automatically generated (based on a schema) and manually generated test cases against an endpoint.</dc:description>
    <dc:rights xml:lang="en">This ontology is licensed under Apache 2.0 (http://www.apache.org/licenses/LICENSE-2.0) and CC-BY (http://creativecommons.org/licenses/by/3.0/)</dc:rights>
    <dcterms:title xml:lang="en">Test-Driven Data Debugging Ontology</dcterms:title>
    <ns1:changes xmlns:ns1="http://purl.org/vocab/vann/" rdf:resource="https://github.com/AKSW/Databugger/commits/master/ontology"/>
    <ns2:preferredNamespacePrefix xmlns:ns2="http://purl.org/vocab/vann/">tddo</ns2:preferredNamespacePrefix>
    <ns3:preferredNamespaceUri xmlns:ns3="http://purl.org/vocab/vann/">http://databugger.aksw.org/ns/core#</ns3:preferredNamespaceUri>
    <rdfs:comment xml:lang="en">The Databugger ontology describes concepts used in Databugger, a test driven data-debugging framework that can run automatically generated (based on a schema) and manually generated test cases against an endpoint.</rdfs:comment>
    <rdfs:seeAlso rdf:resource="http://aksw.org/Projects/Databugger.html"/>
  </owl:Ontology>
  <ns4:TestAppliesTo xmlns:ns4="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#Application">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">Application: When the tests are specific to an application only</rdfs:comment>
    <rdfs:label xml:lang="en">Application</rdfs:label>
  </ns4:TestAppliesTo>
  <ns5:TestGenerationType xmlns:ns5="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#AutoGenerated">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">AutoGenerated: This test was created automatically</rdfs:comment>
    <rdfs:label xml:lang="en">AutoGenerated</rdfs:label>
  </ns5:TestGenerationType>
  <ns6:ParameterConstraint xmlns:ns6="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#Class">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">The parameter is restricted to a Class.</rdfs:comment>
    <rdfs:label xml:lang="en">Class</rdfs:label>
  </ns6:ParameterConstraint>
  <ns7:TestAppliesTo xmlns:ns7="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#Dataset">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">Dataset: When the tests applies to a dataset only (i.e. a SPARQL Endpoint)</rdfs:comment>
    <rdfs:label xml:lang="en">Dataset</rdfs:label>
  </ns7:TestAppliesTo>
  <ns8:TestAppliesTo xmlns:ns8="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#EnrichedSchema">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">EnrichedSchema: When the tests applies to an ontology / vocabulary enriched with external (semi-)automatic approaches</rdfs:comment>
    <rdfs:label xml:lang="en">EnrichedSchema</rdfs:label>
  </ns8:TestAppliesTo>
  <ns9:TestGenerationType xmlns:ns9="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#ManuallyGenerated">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">ManuallyGenerated: This test was created manually</rdfs:comment>
    <rdfs:label xml:lang="en">ManuallyGenerated</rdfs:label>
  </ns9:TestGenerationType>
  <ns10:ParameterConstraint xmlns:ns10="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#None">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">The parameter can be anything, even a free variable (?var)</rdfs:comment>
    <rdfs:label xml:lang="en">None</rdfs:label>
  </ns10:ParameterConstraint>
  <ns11:ParameterConstraint xmlns:ns11="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#Operator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">The parameter can be an operator.</rdfs:comment>
    <rdfs:label xml:lang="en">Operator</rdfs:label>
  </ns11:ParameterConstraint>
  <owl:Class rdf:about="http://databugger.aksw.org/ns/core#Parameter">
    <rdfs:label xml:lang="en">Parameter</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://databugger.aksw.org/ns/core#ParameterConstraint">
    <rdfs:label xml:lang="en">ParameterConstraint</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://databugger.aksw.org/ns/core#Pattern">
    <rdfs:label xml:lang="en">Pattern</rdfs:label>
  </owl:Class>
  <ns12:ParameterConstraint xmlns:ns12="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#Property">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">The parameter is restricted to a property.</rdfs:comment>
    <rdfs:label xml:lang="en">Property</rdfs:label>
  </ns12:ParameterConstraint>
  <ns13:ParameterConstraint xmlns:ns13="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#Resource">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">The pattern can be a resource.</rdfs:comment>
    <rdfs:label xml:lang="en">Resource</rdfs:label>
  </ns13:ParameterConstraint>
  <owl:Class rdf:about="http://databugger.aksw.org/ns/core#Result">
    <rdfs:comment xml:lang="en">A class that holds the results of a single test execution</rdfs:comment>
    <rdfs:label xml:lang="en">Result</rdfs:label>
  </owl:Class>
  <ns14:TestAppliesTo xmlns:ns14="http://databugger.aksw.org/ns/core#" rdf:about="http://databugger.aksw.org/ns/core#Schema">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">Schema</rdfs:label>
  </ns14:TestAppliesTo>
  <owl:Class rdf:about="http://databugger.aksw.org/ns/core#Test">
    <rdfs:comment xml:lang="en">A Test is a testcase in which all properties of the test are described.</rdfs:comment>
    <rdfs:label xml:lang="en">Test</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://databugger.aksw.org/ns/core#TestAppliesTo">
    <rdfs:label xml:lang="en">TestAppliesTo</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://databugger.aksw.org/ns/core#TestGenerationType">
    <rdfs:label xml:lang="en">TestGenerationType</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://databugger.aksw.org/ns/core#TestGenerator">
    <rdfs:label xml:lang="en">TestGenerator</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://databugger.aksw.org/ns/core#appliesTo">
    <rdfs:comment xml:lang="en">applies to describes the ontology used for terms in the Test.</rdfs:comment>
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Test"/>
    <rdfs:label xml:lang="en">appliesTo</rdfs:label>
    <rdfs:range rdf:resource="http://databugger.aksw.org/ns/core#TestAppliesTo"/>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://databugger.aksw.org/ns/core#basedOnPattern">
    <rdfs:comment xml:lang="en">basedOnPattern contains the Data Quality Test Pattern that is instantiated in the described Test.</rdfs:comment>
    <rdfs:label xml:lang="en">basedOnPattern</rdfs:label>
    <rdfs:range rdf:resource="http://databugger.aksw.org/ns/core#Pattern"/>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://databugger.aksw.org/ns/core#generated">
    <rdfs:comment xml:lang="en">generated specifies the way the test was constructed.</rdfs:comment>
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Test"/>
    <rdfs:label xml:lang="en">generated</rdfs:label>
    <rdfs:range rdf:resource="http://databugger.aksw.org/ns/core#TestGenerationType"/>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://databugger.aksw.org/ns/core#parameter">
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Pattern"/>
    <rdfs:label xml:lang="en">parameter</rdfs:label>
    <rdfs:range rdf:resource="http://databugger.aksw.org/ns/core#Parameter"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://databugger.aksw.org/ns/core#parameterConstraint">
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Parameter"/>
    <rdfs:label xml:lang="en">parameterConstraint</rdfs:label>
    <rdfs:range rdf:resource="http://databugger.aksw.org/ns/core#ParameterConstraint"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://databugger.aksw.org/ns/core#references">
    <rdfs:comment xml:lang="en">references specifies the Classes and Properties involved in the described testcase.</rdfs:comment>
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Test"/>
    <rdfs:label xml:lang="en">references</rdfs:label>
    <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://databugger.aksw.org/ns/core#resultCount">
    <rdfs:comment xml:lang="en">The error results of a test execution</rdfs:comment>
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Result"/>
    <rdfs:label xml:lang="en">resultCount</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://databugger.aksw.org/ns/core#resultPrevalence">
    <rdfs:comment xml:lang="en">The prevalence result of a test execution</rdfs:comment>
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Result"/>
    <rdfs:label xml:lang="en">resultPrevalence</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://databugger.aksw.org/ns/core#selectVariable">
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Pattern"/>
    <rdfs:label xml:lang="en">selectVariable</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://databugger.aksw.org/ns/core#source">
    <rdfs:comment xml:lang="en">source is a URI that points to a general Source, which can be a schema, a dataset or an application</rdfs:comment>
    <rdfs:label xml:lang="en">source</rdfs:label>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://databugger.aksw.org/ns/core#sparql">
    <rdfs:comment xml:lang="en">A sparql query</rdfs:comment>
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Test"/>
    <rdfs:label xml:lang="en">sparql</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://databugger.aksw.org/ns/core#sparqlGenerator">
    <rdfs:comment xml:lang="en">A SPARQL query used to instantiate test cases based on patterns</rdfs:comment>
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#TestGenerator"/>
    <rdfs:label xml:lang="en">sparqlGenerator</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://databugger.aksw.org/ns/core#sparqlPattern">
    <rdfs:comment xml:lang="en">A pattern sparql query</rdfs:comment>
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Pattern"/>
    <rdfs:label xml:lang="en">sparqlPattern</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://databugger.aksw.org/ns/core#sparqlPrevalence">
    <rdfs:comment xml:lang="en">A sparql query for prevalence</rdfs:comment>
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Test"/>
    <rdfs:label xml:lang="en">sparqlPrevalence</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://databugger.aksw.org/ns/core#sparqlPrevalencePattern">
    <rdfs:comment xml:lang="en">A pattern sparql prevalence query</rdfs:comment>
    <rdfs:domain rdf:resource="http://databugger.aksw.org/ns/core#Pattern"/>
    <rdfs:label xml:lang="en">sparqlPrevalencePattern</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://databugger.aksw.org/ns/core#test">
    <rdfs:comment>points to a Test</rdfs:comment>
    <rdfs:label xml:lang="en">test</rdfs:label>
    <rdfs:range rdf:resource="http://databugger.aksw.org/ns/core#Test"/>
  </owl:ObjectProperty>
  <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#cardinality"/>
  <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#minCardinality"/>
</rdf:RDF>
