@prefix sh:      <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://example.com/arguments/1> a sh:Parameter ;
	sh:path sh:arg1 ;
	sh:datatype xsd:integer ;
	rdfs:comment "The first operand" ;
    .

<http://example.com/arguments/2> a sh:Parameter ;
	sh:path sh:arg2 ;
	sh:datatype xsd:integer ;
	rdfs:comment "The second operand" ;
	.


<http://example.com/arguments/3> a sh:Parameter ;
	sh:path sh:arg3 ;
	sh:datatype xsd:integer ;
	sh:defaultValue "42"^^xsd:integer ;
	sh:optional true ;
	rdfs:comment "The third operand" ;
	.
