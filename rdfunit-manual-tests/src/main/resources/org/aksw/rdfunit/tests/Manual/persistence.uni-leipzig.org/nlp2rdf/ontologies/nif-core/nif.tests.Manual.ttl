                            } """ ;
    rut:resultAnnotation [
        a rut:ResultAnnotation ;
        rut:annotationProperty spin:violationPath ;
        rut:annotationValue <http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#referenceContext>
    ];
    .

